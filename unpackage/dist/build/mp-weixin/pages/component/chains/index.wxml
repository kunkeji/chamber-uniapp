<view class="component-chains" scroll-x="{{true}}"><block wx:for="{{showData}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toDetails',['$0'],[[['showData','id',item.id]]]]]]]}}" class="{{['chains-item',(showType==1)?'chains-list-item':'']}}" bindtap="__e"><view class="item-title text-ellipsis">{{item.name}}</view><view class="item-tag flex justify-content-between"><view class="tag-box flex"><text style="margin-left:0;">{{item.expire_time}}</text><block wx:if="{{item.type==1}}"><text>自由接龙</text></block><block wx:else><text>限定接龙</text></block></view><view class="tag-box flex"><text>{{"浏览"+item.page_view}}</text><text>{{"参与"+item.part_total}}</text></view></view><view class="item-btn flex align-items-center"><button class="btn-box clear flex align-items-center" open-type="share" data-event-opts="{{[['tap',[['setShareData',['$0'],[[['showData','id',item.id]]]]]]]}}" catchtap="__e"><block wx:if="{{iconInvite}}"><view class="icon" style="{{'background-image:'+('url('+iconInvite+')')+';'}}"></view></block><text class="text">邀请填写</text></button><view class="btn-line"></view><view data-event-opts="{{[['tap',[['onContact',['$0'],[[['showData','id',item.id,'mobile']]]]]]]}}" class="btn-box justify-content-end flex align-items-center" catchtap="__e"><block wx:if="{{iconPhone}}"><view class="icon" style="{{'background-image:'+('url('+iconPhone+')')+';'}}"></view></block><text class="text">联系电话</text></view></view></view></block></view>