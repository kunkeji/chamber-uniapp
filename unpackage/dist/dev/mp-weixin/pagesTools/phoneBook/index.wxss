.container .container-main .main-search {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 99;
  background: #ffffff;
  padding: 16rpx 32rpx;
}
.container .container-main .main-search .main-search-box {
  padding: 20rpx 32rpx;
  border-radius: 16rpx;
  background: #F9F9F9;
  color: #999999;
}
.container .container-main .main-search .main-search-box .box-img {
  width: 40rpx;
  height: 40rpx;
}
.container .container-main .main-search .main-search-box .box-txt {
  padding-left: 24rpx;
  font-size: 32rpx;
}
.container .container-main .main-search .main-search-box .box-txt .placeholder {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #BBBBBB;
}
.container .container-main .main-search .main-search-right {
  padding-left: 32rpx;
}
.container .container-main .main-search .main-search-right .right-icon {
  width: 40rpx;
  height: 40rpx;
  background-size: 40rpx;
}
.container .container-main .main-search .main-search-right .right-txt {
  padding-left: 8rpx;
  font-size: 28rpx;
}
.container .container-main .main-phoneList {
  margin: 32rpx;
  padding: 32rpx;
  border-radius: 12rpx;
  background: #FFFFFF;
}
.container .container-main .main-phoneList .main-phoneList-title {
  padding-bottom: 32rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box {
  padding-bottom: 32rpx;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-img {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-personal {
  flex: 1;
  padding-left: 32rpx;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-personal .info-personal-name {
  font-size: 32rpx;
  font-weight: 600;
  line-height: 44rpx;
  color: #5A5B6E;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-personal .info-personal-level {
  padding-left: 16rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #8D929C;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-personal .info-personal-phone {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-icon {
  padding: 8rpx;
  border-radius: 50%;
  background: var(--theme-color);
}
.container .container-main .main-phoneList .main-phoneList-info .info-box .info-icon image {
  width: 48rpx;
  height: 48rpx;
}

