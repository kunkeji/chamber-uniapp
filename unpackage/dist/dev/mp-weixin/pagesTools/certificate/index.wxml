<view><title-bar vue-id="f5154a94-1" title="{{navigationBarTitle}}" bind:__l="__l"></title-bar><view class="container"><scroll-view class="tab-scroll" scroll-x="{{true}}"><view class="tab-bar"><block wx:for="{{venueTypes}}" wx:for-item="type" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['filterVenues',[index]]]]]}}" class="{{['tab-item',(selectedType===index)?'active':'']}}" bindtap="__e">{{''+type.name+''}}</view></block></view></scroll-view><view class="venue-list"><block wx:for="{{filteredVenues}}" wx:for-item="venue" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['goToDetails',['$0'],[[['filteredVenues','id',venue.id]]]]]]]}}" class="venue-card" bindtap="__e"><view class="venue-image-wrapper"><image class="venue-image" src="{{venue.image}}" mode="aspectFill"></image></view><view class="venue-info"><view class="venue-title">{{venue.name}}</view><view class="venue-price">{{venue.price!=0.00?venue.price+"元/小时":"免费"}}</view><view class="venue-details"><text class="venue-size">{{venue.size+"㎡"}}</text><text class="venue-location">{{venue.location}}</text></view></view></view></block></view></view></view>