.container .container-main {
  padding: 32rpx 32rpx 144rpx;
}
.container .container-main .main-sequence-info {
  padding: 32rpx;
  margin-bottom: 32rpx;
  border-radius: 12rpx;
  background: #FFFFFF;
}
.container .container-main .main-sequence-info .sequence-info-title {
  margin-bottom: 32rpx;
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight: 600;
  color: #5A5B6E;
}
.container .container-main .main-sequence-info .sequence-info-person {
  margin-bottom: 32rpx;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-item {
  flex: 1;
  padding-left: 16rpx;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-item .info-person-item-name {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-item .info-person-item-level {
  padding-left: 8rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-item .info-person-item-time {
  font-size: 24rpx;
  line-height: 34rpx;
  color: #8D929C;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-button {
  padding: 12rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  border-radius: 8rpx;
  color: #FFFFFF;
}
.container .container-main .main-sequence-info .sequence-info-person .info-person-button .info-person-button-icon {
  padding-right: 8rpx;
  width: 32rpx;
  height: 32rpx;
}
.container .container-main .main-sequence-info .sequence-info-time {
  margin-bottom: 32rpx;
  padding: 16rpx 0rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #5A5B6E;
  border-radius: 8rpx;
  text-align: center;
  background: #EEF2FF;
}
.container .container-main .main-sequence-info .sequence-info-introduce {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #8D929C;
}
.container .container-main .main-sequence-situation {
  padding: 32rpx;
  border-radius: 10rpx;
  background: #FFFFFF;
}
.container .container-main .main-sequence-situation .situation-title {
  margin-bottom: 16rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
}
.container .container-main .main-sequence-situation .situation-list {
  display: flex;
  flex-wrap: wrap;
  margin-left: -16rpx;
}
.container .container-main .main-sequence-situation .situation-list .situation-list-radio {
  width: 16rpx;
  height: 16rpx;
  background: var(--theme-color);
  border-radius: 50%;
  position: absolute;
  top: -2rpx;
  right: -2rpx;
}
.container .container-main .main-sequence-situation .situation-list .situation-list-item {
  margin: 16rpx 0 0 16rpx;
  padding: 12rpx 16rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
  border-radius: 8rpx;
  background: #F6F7FB;
}
.container .container-main .main-sequence-situation .situation-list .situation-list-item-finish {
  margin: 16rpx 0 0 16rpx;
  padding: 11rpx 15rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #5A5B6E;
  border-radius: 8rpx;
  background: #F6F7FB;
  border: 1rpx solid var(--theme-color);
  position: relative;
  z-index: 1;
}
.container .container-main .main-sequence-situation .situation-list .situation-list-empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  font-size: 28rpx;
  padding-bottom: 16rpx;
  color: #8D929C;
}
.container .container-main .main-sequence-situation .situation-list .situation-list-empty .list-empty-img {
  width: 128rpx;
  height: 128rpx;
}
.container .container-main .main-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  text-align: center;
  background: #FFF;
  padding: 16rpx 24rpx;
  z-index: 1;
}
.container .container-main .main-bottom .main-bottom-phone {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .container-main .main-bottom .main-bottom-phone .phoneTxt {
  font-size: 20rpx;
  line-height: 28rpx;
}
.container .container-main .main-bottom .main-bottom-phone .phoneImg {
  width: 52rpx;
  height: 52rpx;
}
.container .container-main .main-bottom .main-bottom-button {
  flex: 1;
  margin-left: 36rpx;
  padding: 22rpx 0rpx 22rpx;
  background: #999999;
  color: white;
  text-align: center;
  line-height: 44rpx;
  font-size: 32rpx;
  border-radius: 16rpx;
}
.container .container-main .main-share {
  padding: 32rpx 48rpx;
  border-radius: 40rpx 40rpx 0rpx 0rpx;
  background: #FFFFFF;
  width: 100vw;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
}
.container .container-main .main-share .main-share-title {
  text-align: center;
  font-size: 32rpx;
  line-height: 40rpx;
  color: #5A5B6E;
}
.container .container-main .main-share .main-share-item {
  margin-top: 64rpx;
  padding: 0rpx 128rpx 0rpx 128rpx;
  display: flex;
  justify-content: space-between;
}
.container .container-main .main-share .main-share-item .share-item-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 0rpx;
  padding: 0rpx;
  background-color: transparent;
}
.container .container-main .main-share .main-share-item .share-item-box .item-box-txt {
  padding-top: 16rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #8D929C;
}
.container .container-main .main-share .main-share-item .share-item-box .item-box-img {
  width: 96rpx;
  height: 96rpx;
}
.container .container-main .main-share .main-share-cancel {
  margin-top: 36rpx;
  text-align: center;
  font-size: 32rpx;
  line-height: 40rpx;
  color: #E10602;
}
.container .container-main .main-login {
  padding: 96rpx 60rpx 0;
}
.container .container-main .main-login .login-image {
  width: 100%;
  height: 500rpx;
}
.container .container-main .main-login .login-tips {
  color: #585858;
  font-size: 36rpx;
  line-height: 50rpx;
  margin-top: 48rpx;
  text-align: center;
}
.container .container-main .main-login .login-btn {
  margin-top: 56rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  border-radius: 16rpx;
  text-align: center;
  background: var(--theme-color);
  color: #ffffff;
}
.container .container-main .main-login .login-btn.cancel {
  background: #dedede;
  color: #999;
  margin-top: 48rpx;
}

