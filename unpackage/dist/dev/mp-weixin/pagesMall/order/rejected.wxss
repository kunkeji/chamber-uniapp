.container .container-main {
  padding-bottom: 144rpx;
}
.container .container-main .main-goods {
  margin: 32rpx;
}
.container .container-main .main-trackingNumber {
  margin: 32rpx;
  border-radius: 20rpx;
  padding: 32rpx;
  background: #FFF;
}
.container .container-main .main-trackingNumber .trackingNumber-title {
  color: #5A5B6E;
  font-size: 28rpx;
  font-weight: 600;
}
.container .container-main .main-trackingNumber .trackingNumber-code {
  margin-top: 24rpx;
  color: #999;
  font-size: 28rpx;
  border-radius: 10rpx;
  padding: 20rpx 32rpx;
  background: #F6F7FB;
}
.container .container-main .main-trackingNumber .placeholder-class {
  color: #999;
  font-size: 28rpx;
}
.container .container-main .main-bottom {
  padding-top: 16rpx;
  padding-bottom: 16rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  text-align: center;
  background: #FFF;
  z-index: 99;
}
.container .container-main .main-bottom .harmless-content-button {
  margin-left: 26rpx;
  margin-right: 26rpx;
  width: 700rpx;
  height: 80rpx;
  background: #325dff;
  color: white;
  text-align: center;
  line-height: 80rpx;
  border-radius: 16rpx;
}
.container .container-main .main-bottom .safe-padding {
  width: 100%;
  padding-bottom: constant(safe-area-inset-bottom);
  /* 兼容 iOS < 11.2 */
  padding-bottom: env(safe-area-inset-bottom);
  /* 兼容 iOS >= 11.2 */
}

