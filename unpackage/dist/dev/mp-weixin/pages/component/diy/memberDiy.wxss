.diy-member .member-title {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.diy-member .member-scroll {
  white-space: nowrap;
}
.diy-member .member-scroll .scroll-item {
  display: inline-block;
}
.diy-member .member-scroll .scroll-item:first-child {
  margin-left: 0 !important;
}
.diy-member .member-scroll .scroll-item .item-label {
  line-height: 1;
  padding: 10rpx 8rpx;
  margin-top: -16rpx;
  text-align: center;
  position: relative;
  z-index: 1;
}
.diy-member .member-scroll .scroll-item .item-name {
  color: #5A5B6E;
  line-height: 1;
  margin-top: 18rpx;
  text-align: center;
}
.diy-member .member-swiper .swiper-item .item-label {
  line-height: 1;
  padding: 10rpx 8rpx;
  margin-top: -16rpx;
  text-align: center;
  position: relative;
  z-index: 1;
}
.diy-member .member-swiper .swiper-item .item-name {
  color: #5A5B6E;
  line-height: 1;
  margin-top: 18rpx;
  text-align: center;
}

